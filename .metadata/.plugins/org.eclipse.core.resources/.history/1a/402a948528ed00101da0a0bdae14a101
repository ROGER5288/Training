package Operators.CompoundAssignmentoperators;

public class Program1 {
	public static void main(String[] args) {
		// this operator is an implicit typecaster
		int a=100;
		
		a+=10; //a= a+10;
		
		a+=10.5; //a = a + 10.5 error will show
		
		System.out.println(a);
		System.out.println();
		System.out.println();
		System.out.println();
	}

}
