package User_Input;


public class Program3 {
	public static void main(String [] args) {
		
		
		//Jagged array
		int [][]a= new int [2][];
		a[0] =new int [3];
		a[1] =new int [2];
		a[2] =new int [5];
		
		for(int i = 0; i<a.length;i++) {
			for(int j = 0; j<a[i].length;j++) {
				System.out.println(a[i][j]+" ");
				
			}
			System.out.println();
		}
		
		
	}
	
	
}
