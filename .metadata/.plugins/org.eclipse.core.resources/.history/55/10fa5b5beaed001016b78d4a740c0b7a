package Methods;

public class Program1 {
	public static void main(String[]args) {
		demo();//method signature
		test(10);
		//no argument method
		//parameetrized method
	}
	public static void demo() {
		System.out.println("Demo");
	}
	public static void test(int a) {
		System.out.println("Test")
	}
}
