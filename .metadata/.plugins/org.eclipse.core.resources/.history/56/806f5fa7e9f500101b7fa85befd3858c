package Object_Demo;

public class Student {
	int id;
	String name;
	String email;

	public Student(int id, String name, String email) {
		this.id = id;
		this.name = name;
		this.email = email;
	}

	
	public boolean equals(Object obj) {
		Employee em = (Employee) obj;
		
		if(em.id == this.id ) {
			return true;
		}else {
			return false;
		}
	}
}