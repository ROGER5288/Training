package String.Basics;

public class Program9 {
	public static void main(String[] args) {
		
		// "\\d" to check if it's a digit.
		System.out.println("1".matches("\\d"));
		System.out.println("13456".matches("[6-9]\\d+"));

		System.out.println("83456".matches("[6-9]\\d+"));
		
		System.out.println("9854113456".matches("[6-9]\\d{9,}"));//9 and above
		
		System.out.println("9854113456".matches("[6-9]\\d{9,12}"));// min 9 and max 12
		System.out.println("98541134563265421".matches("[6-9]\\d{9,12}"));		
		
	}
}
