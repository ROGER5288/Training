package Methods;

public class Program11 {
	public static void main(String[] args) {
		int n = 5;
		for ( int row = 1; row <= n; row++) {
			for(int col = 1; col<=2*n-1;col++) {
				if(col<=row || row+col >= 2*n && col+row <=n+1+2 || row+n+1<=col) {
					System.out.print("*");
				}else{
					System.out.print(" ");
				}
				
			}
			System.out.println();
		}
	}
}
