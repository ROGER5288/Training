package FlowControlStatement.LoopingStatement.Forloop;

public class Program3 {
	public static void main(String[]args) {
		for(int i = 0; i<3; i++) {
			for(int j =0;j<3;j++) {
				System.out.print("*");   
			}
			System.out.println("")
		}
		/*
		for(int i =0;i<3;i++) {
			System.out.print("*");   
			//* * *
		}
		for(int i =0;i<3;i++) {
			System.out.println("*");
			//***
			//***
		}
		for(int i =0;i<3;i++) {
			System.out.println("*");
			//***
			//***
			//***
		}
		*/
		
	}
}
