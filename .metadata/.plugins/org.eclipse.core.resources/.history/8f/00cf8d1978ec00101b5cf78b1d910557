package Programs_on_variables;

public class Program9 {
	public static void main(String[] args) {
		//byte b = 127;
		//byte b = (byte)128;
		byte b = (byte)129;
		System.out.println(b);
		/*
		  -128 --->0<--- 127
		  if we cast typecast operator
		  128---> -128
		  129---> -127
		  130---> -126
		 */
		
		int j = 1_00_0;
		
		System.out.println(j);
		
		int k = 1_4;
		
		System.out.println(k);
	}
}
