package OOP.Fundamentals.Non_StaticInitializer;

public class Person {
	String name;//null
	int age;//0
	String gender;//null
	
	public void display(){	
		System.out.println("Name : " + name);
		System.out.println("Age : " + age);
		System.out.println("Gender : " + gender);
	}
	public void initialize(String name, int age, String gender) {
		
	}
}
