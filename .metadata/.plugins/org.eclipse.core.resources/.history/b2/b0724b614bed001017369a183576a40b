package FlowControlStatement.LoopingStatement.Forloop;

public class Program2 {
	public static void main(String[]args) {
		int num = 10;
		int j = 0;
		for( int i = 1; i<=num;i++) {
			if(num%i==0) {
				j++;
			}
		}
		if(j==2) {
			System.out.println("Prime Number");
		}else {
			System.out.println("Non-XjPrime Number");
		}
	}

}
