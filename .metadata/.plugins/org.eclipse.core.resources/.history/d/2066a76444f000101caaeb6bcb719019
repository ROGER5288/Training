package Methods;

public class Program9 {
	public static void main(String[] args) {
		 num( 132);
	}
	public static int num(int a) {
		int temp = a;
		int n=0;
		while(temp!=0) {
			int b =(temp%10);
			temp/=10;
			n= fact(b)+n;
		}
		
	}
	public static int fact(int n) {
		int res = 1;
		for(int i = n; i>=1;i--) {
			res*=i;
		}
		return res;
	}
}
