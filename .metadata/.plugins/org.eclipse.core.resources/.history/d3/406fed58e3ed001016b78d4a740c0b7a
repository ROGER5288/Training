package FlowControlStatement.LoopingStatement.Forloop;

public class Program7 {
	public static void main(String[] args) {
		int n = 3;
		for(int row =1; row <=3; row++) {
			for(int col = 1; col <=3; col++) {
				if(row+col>=n+1) {
					System.out.print("*");
				}else {
					System.out.print(" ");
				}
				
			}
			System.out.println("");
		}
	}
}
