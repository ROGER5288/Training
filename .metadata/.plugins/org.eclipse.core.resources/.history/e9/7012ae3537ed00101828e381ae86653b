package FlowControlStatement.DecisionStatement;

public class Program5 {
	public static void main(String[] args) {
		
		// datatypes taken by swtich are int, short, byte, char, string
		
		byte b = 1;
		
		switch(b) {
		case 1:
			System.out.println("1");
			break; // without the break the switch will consider evrery other options after that perticular one
		case 2:
			System.out.println(2);
			break;
		case 3:
			System.out.println(3);
			break;
		default:
			System.out.println("Default");
			break;
		}
		
		
	}

}
