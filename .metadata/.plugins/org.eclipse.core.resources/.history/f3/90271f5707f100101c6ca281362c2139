package OOP.Fundamentals.Non_StaticInitializer;

public class Program2 {
	int a;
	double d;
	//char c;
	byte b;
	String s;
	public static void main(String[] args) {
//		System.out.println(new Program2().a);
//		System.out.println(new Program2().b);
//		System.out.println(new Program2().d);
//		System.out.println(new Program2().s);
		// 4 different objects has been created to access the non-static variable 
		
		Program2 obj1=new Program2();
		System.out.println(obj1);
		
		// in obj1 we can only store two type of value 1st is null and 2nd is object reference
		
	}

}
