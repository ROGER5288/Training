package Programs_on_variables;

public class Program1 {
	
	//variable block of memory data
	// datatype identifier [Syntax]
	public static void main(String[] args) {
		/* byte a;
		a=127;
		a = -128 no issues
		 * a=130; error type mismatched
		 * a=-129 same error
		 * a=128 same error
		 */
		/*short a;
		a=129;
		*/
		/*int a;
		 * a=2000;
		 */
		/* long a;
		 *  a=100000000000l;     [ for long diclaration we need to put l after the number like a=100000000l ]
		 */
		long a;
		a=10000000000l;
		System.out.println(a);
		System.out.println();
		
	}

}
